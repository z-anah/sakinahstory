{
  "name": "react-tailwind",
  "version": "6.0.0",
  "description": "React and Tailwind CSS Starter Kit",
  "keywords": [
    "vite",
    "react",
    "tailwindcss"
  ],
  "license": "MIT",
  "type": "module",
  "homepage": "https://z-anah.github.io/sakinahstory/",
  "scripts": {
    "build": "rm -rf docs && vite build",
